package java21;
import java.util.Scanner;
public class vidu {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		for( int a=1; a<=20; a++) {
			System.out.println(a+ " NguyenVanTy");
		}
		for(int i=2;i<=9;i++) {
			for(int j=1;j<=10;i++) {
				System.out.println(i+"x"+j+"="+(i*j));
			}
			System.out.println("---");
		}
		
	}

}
